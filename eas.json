{
  "cli": {
    "version": ">= 5.9.0",
    "appVersionSource": "local"
  },
  "build": {
    "production": {
      "distribution": "store",
      "env": {
        "APP_VARIANT": "production"
      },
      "android": {
        "buildType": "app-bundle"
      },
      "ios": {
        "resourceClass": "m-medium"
      }
    },
    "preview": {
      "distribution": "internal",
      "env": {
        "APP_VARIANT": "preview"
      },
      "android": {
        "buildType": "apk"
      },
      "ios": {
        "resourceClass": "m-medium"
      }
    },
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "env": {
        "APP_VARIANT": "development"
      }
    }
  },
  "submit": {
    "production": {
      "ios": {
        "appleId": "ЗАПОЛНИТЕ_ВАШ_APPLE_ID@mail.com",
        "ascAppId": "ЗАПОЛНИТСЯ_ПОСЛЕ_СОЗДАНИЯ_APP",
        "appleTeamId": "ЗАПОЛНИТСЯ_АВТОМАТИЧЕСКИ"
      },
      "android": {
        "serviceAccountKeyPath": "./google-play-service-account.json",
        "track": "internal"
      }
    }
  }
}
