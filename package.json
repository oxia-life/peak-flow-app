{
  "name": "peak-flow-diary",
  "version": "1.0.0",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "build:web": "expo export --platform web && cp -r public/* dist/",
    "deploy:web": "npm run build:web && npx serve dist",
    "mobile:preview": "eas build --platform all --profile preview",
    "mobile:build:ios": "eas build --platform ios --profile production",
    "mobile:build:android": "eas build --platform android --profile production",
    "mobile:build:all": "eas build --platform all --profile production",
    "mobile:submit:ios": "eas submit --platform ios",
    "mobile:submit:android": "eas submit --platform android",
    "mobile:submit:all": "eas submit --platform all",
    "safe-push": "bash scripts/safe-push.sh",
    "check:branch": "git branch --show-current"
  },
  "dependencies": {
    "@expo/metro-runtime": "~6.1.2",
    "@react-native-async-storage/async-storage": "2.2.0",
    "@react-native-community/datetimepicker": "8.4.4",
    "@react-navigation/bottom-tabs": "^6.5.11",
    "@react-navigation/native": "^6.1.9",
    "@react-navigation/native-stack": "^6.9.17",
    "@shopify/react-native-skia": "2.2.12",
    "@supabase/supabase-js": "^2.84.0",
    "@vercel/analytics": "^1.5.0",
    "expo": "^54.0.27",
    "expo-build-properties": "~1.0.10",
    "expo-linear-gradient": "~15.0.8",
    "expo-splash-screen": "~31.0.12",
    "expo-sqlite": "~16.0.10",
    "expo-status-bar": "~3.0.9",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.5",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-reanimated": "~4.1.1",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-svg": "15.12.1",
    "react-native-web": "^0.21.0",
    "react-native-worklets": "^0.7.1",
    "victory-native": "^41.20.2"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@types/react": "~19.1.10",
    "typescript": "~5.9.2"
  },
  "private": true
}
