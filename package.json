{
  "name": "peak-flow-diary",
  "version": "1.0.0",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "build:web": "expo export --platform web && cp -r public/* dist/",
    "deploy:web": "npm run build:web && npx serve dist",
    "mobile:preview": "eas build --platform all --profile preview",
    "mobile:build:ios": "eas build --platform ios --profile production",
    "mobile:build:android": "eas build --platform android --profile production",
    "mobile:build:all": "eas build --platform all --profile production",
    "mobile:submit:ios": "eas submit --platform ios",
    "mobile:submit:android": "eas submit --platform android",
    "mobile:submit:all": "eas submit --platform all",
    "safe-push": "bash scripts/safe-push.sh",
    "check:branch": "git branch --show-current"
  },
  "dependencies": {
    "@expo/metro-runtime": "~3.2.3",
    "@react-native-async-storage/async-storage": "1.23.1",
    "@react-native-community/datetimepicker": "^8.0.1",
    "@react-navigation/bottom-tabs": "^6.5.11",
    "@react-navigation/native": "^6.1.9",
    "@react-navigation/native-stack": "^6.9.17",
    "@supabase/supabase-js": "^2.84.0",
    "@vercel/analytics": "^1.5.0",
    "expo": "~51.0.28",
    "expo-linear-gradient": "~13.0.2",
    "expo-sqlite": "~14.0.6",
    "expo-status-bar": "~1.12.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.74.5",
    "react-native-safe-area-context": "4.10.5",
    "react-native-screens": "~3.31.1",
    "react-native-svg": "15.2.0",
    "react-native-web": "~0.19.10",
    "victory-native": "^41.20.2"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@types/react": "~18.2.45",
    "typescript": "~5.3.3"
  },
  "private": true
}
